import re
check = re.compile(r"^ +( )  $")
with open("ascii_arts.txt", "r") as file:
    ascii_art = ""
    print("\\[")
    for line in file.readlines():
        ascii_art = line.rstrip("\n")
        if re.match(check, ascii_art):
            ascii_art = ''
        if ascii_art[0:4] == "+---":
            print("\\],")
            print("\\[")
        elif ascii_art != '':
            ascii_art = ascii_art.replace("'", "`")
            print("\\'" + ascii_art + "',")
    print("\\],")
